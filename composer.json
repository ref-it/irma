{
    "name": "openadministration/stumv",
    "type": "project",
    "version": "1.1.0",
    "description": "ldap based membership management for student bodys",
    "keywords": ["ldap", "laravel", "memership-management", "student-body"],
    "license": "AGPL",
    "require": {
        "php": "^8.2",
        "ext-ldap": "*",
        "ext-pdo": "*",
        "dacoto/laravel-domain-validation": "^3.0",
        "diglactic/laravel-breadcrumbs": "^8.1",
        "directorytree/ldaprecord-laravel": "^3.0",
        "directorytree/ldaprecord": "v3.5.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.10",
        "livewire/livewire": "^3.0",
        "owenvoke/blade-fontawesome": "^2.0"
    },
    "require-dev": {
        "laravel/tinker": "^2.7",
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.9",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.10",
        "phpunit/phpunit": "^9.5.10",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
